int foo(int param)
{
	// These definitions test integer, float, char, string and bool literals, as well as some reserved words and symbols
	int a = -4; int b = +5; int c = 6;
	float d = 3.1, e = 5.2E+4, f = 0.2e-2;
	char g = 'x';
	string h = "This is my string";

	// These definitions test some identifier names
	int integer_1;
	int _integer2;
	int _3integer;

	/*
	  The next section tests reserved words and composite operators 
	
	  This is a multi-line comment that spans 5 lines total
	*/
	if (param >= a && d == e)
	{
		return 1;
	}
	else
	{
		while (a != b)
		{
			a = a + 1;
		}
		return 0;
	}
}
